{
  "extends": ["standard"],
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "no-var": "warn",
    "no-undef": "off",
    "prefer-const": "warn",
    "prefer-template": "warn",
    "object-shorthand": ["warn", "always"],
    "quote-props": ["warn", "consistent-as-needed"]
  }
}
